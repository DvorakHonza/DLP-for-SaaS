{
    "manifest_version": 3,
    "name": "__MSG_extName__",
    "version": "0.0.1",
    "default_locale": "en",
    "description": "__MSG_extDescription__",
    "icons": {
        "128" : "images/logo_128.png"
    },
    "author": "Jan Dvořák",
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "host_permissions": ["<all_urls>"],
    "permissions": [
        "storage",
        "tabs",
        "nativeMessaging",
        "printerProvider",
        "scripting",
        "webRequest",
        "clipboardRead",
        "clipboardWrite"

    ],
    "storage": {
        "managed_schema": "schema.json"
    }
}